# Description: Example of .env file
# Usage: Copy this file to .env and change the values
#        according to your needs
#        Do not commit .env file to git
#        Do not change .env.example file

# Config directory
# Directory where files, logs and database will be stored
# Example: LANGFLOW_CONFIG_DIR=~/.langflow
LANGFLOW_CONFIG_DIR=

# Save database in the config directory
# Values: true, false
# If false, the database will be saved in Langflow's root directory
# This means that the database will be deleted when Langflow is uninstalled
# and that the database will not be shared between different virtual environments
# Example: LANGFLOW_SAVE_DB_IN_CONFIG_DIR=true
LANGFLOW_SAVE_DB_IN_CONFIG_DIR=

# Database URL
# Postgres example: LANGFLOW_DATABASE_URL=postgresql://postgres:postgres@localhost:5432/langflow
# SQLite example:
LANGFLOW_DATABASE_URL=sqlite:///./langflow.db

# Database connection retry
# Values: true, false
# If true, the database will retry to connect to the database if it fails
# Example: LANGFLOW_DATABASE_CONNECTION_RETRY=true
LANGFLOW_DATABASE_CONNECTION_RETRY=false

# Cache type
LANGFLOW_LANGCHAIN_CACHE=SQLiteCache

# Optional: override components search path (comma-separated absolute paths)
# Example: LANGFLOW_COMPONENTS_PATH=/opt/components,/more/components
LANGFLOW_COMPONENTS_PATH=

# Server host
# Example: LANGFLOW_HOST=localhost
LANGFLOW_HOST=

# Worker processes
# Example: LANGFLOW_WORKERS=1
LANGFLOW_WORKERS=

# Server port
# Example: LANGFLOW_PORT=7860
LANGFLOW_PORT=

# Logging level
# Example: LANGFLOW_LOG_LEVEL=critical
LANGFLOW_LOG_LEVEL=

# Path to the log file
# Example: LANGFLOW_LOG_FILE=logs/langflow.log
LANGFLOW_LOG_FILE=

# Time/Size for log to rotate
# Example: LANGFLOW_LOG_ROTATION=‘10 MB’/‘1 day’
LANGFLOW_LOG_ROTATION=

# Logging environment (e.g. container, local) influences formatting
# Example: LANGFLOW_LOG_ENV=container
LANGFLOW_LOG_ENV=

# Logging format override (e.g. json, text, custom)
LANGFLOW_LOG_FORMAT=

# Pretty logs to stdout (true/false)
LANGFLOW_PRETTY_LOGS=true

# Log retriever buffer size (integer >0 enables in-memory retrieval API)
# Example: LANGFLOW_LOG_RETRIEVER_BUFFER_SIZE=1000
LANGFLOW_LOG_RETRIEVER_BUFFER_SIZE=

# Path to the frontend directory containing build files
# Example: LANGFLOW_FRONTEND_PATH=/path/to/frontend/build/files
LANGFLOW_FRONTEND_PATH=

# Whether to open the browser after starting the server
# Values: true, false
# Example: LANGFLOW_OPEN_BROWSER=true
LANGFLOW_OPEN_BROWSER=

# Whether to remove API keys from the projects saved in the database
# Values: true, false
# Example: LANGFLOW_REMOVE_API_KEYS=false
LANGFLOW_REMOVE_API_KEYS=

# Whether to use RedisCache or ThreadingInMemoryCache or AsyncInMemoryCache
# Values: async, memory, redis
# Example: LANGFLOW_CACHE_TYPE=memory
# If you want to use redis then the following environment variables must be set:
# LANGFLOW_REDIS_HOST (default: localhost)
# LANGFLOW_REDIS_PORT (default: 6379)
# LANGFLOW_REDIS_DB (default: 0)
# LANGFLOW_REDIS_CACHE_EXPIRE (default: 3600)
LANGFLOW_CACHE_TYPE=

# Redis cache settings (used when LANGFLOW_CACHE_TYPE=redis)
# LANGFLOW_REDIS_HOST=localhost
# LANGFLOW_REDIS_PORT=6379
# LANGFLOW_REDIS_DB=0
# LANGFLOW_REDIS_EXPIRE=3600
# LANGFLOW_REDIS_PASSWORD= (optional)
LANGFLOW_REDIS_HOST=
LANGFLOW_REDIS_PORT=
LANGFLOW_REDIS_DB=
LANGFLOW_REDIS_EXPIRE=
LANGFLOW_REDIS_PASSWORD=

# Set LANGFLOW_AUTO_LOGIN to false if you want to disable auto login
# and use the login form to login. LANGFLOW_SUPERUSER and LANGFLOW_SUPERUSER_PASSWORD
# must be set if AUTO_LOGIN is set to false
# Values: true, false
LANGFLOW_AUTO_LOGIN=

# SET LANGFLOW_ENABLE_SUPERUSER_CLI to false to disable
# superuser creation via the CLI
LANGFLOW_ENABLE_SUPERUSER_CLI=

# Superuser username
# Example: LANGFLOW_SUPERUSER=admin
LANGFLOW_SUPERUSER=

# Superuser password
# Example: LANGFLOW_SUPERUSER_PASSWORD=123456
LANGFLOW_SUPERUSER_PASSWORD=

# API key required in some CLI/server contexts when auth enforced
# Example: LANGFLOW_API_KEY=your-generated-key
LANGFLOW_API_KEY=

# Should store environment variables in the database
# Values: true, false
LANGFLOW_STORE_ENVIRONMENT_VARIABLES=

# Disable external telemetry (overrides DO_NOT_TRACK). true/false
LANGFLOW_DO_NOT_TRACK=

# Mark that this is a desktop build (internal usage). true/false
LANGFLOW_DESKTOP=

# Should enable the MCP composer feature in MCP projects
# Values: true, false
# Default: true
LANGFLOW_FEATURE_MCP_COMPOSER=

# Update starter projects automatically (true/false)
LANGFLOW_UPDATE_STARTER_PROJECTS=true

# Polling interval (ms) for filesystem flows auto-load (advanced)
LANGFLOW_FS_FLOWS_POLLING_INTERVAL=

# Deactivate tracing integrations entirely (true/false)
LANGFLOW_DEACTIVATE_TRACING=

# Use in-memory noop database (testing/dev only). 1/true to enable
LANGFLOW_USE_NOOP_DATABASE=

# Automatically load flows from path at startup (absolute path)
LANGFLOW_LOAD_FLOWS_PATH=

# STORE_URL
# Example: LANGFLOW_STORE_URL=https://api.langflow.store
# LANGFLOW_STORE_URL=

# DOWNLOAD_WEBHOOK_URL
#
# LANGFLOW_DOWNLOAD_WEBHOOK_URL=

# LIKE_WEBHOOK_URL
#
# LANGFLOW_LIKE_WEBHOOK_URL=

# Frontend build path override (served statically in packaged mode)
LANGFLOW_FRONTEND_PATH=

# Max file upload size in MB (integer)
LANGFLOW_MAX_FILE_SIZE_UPLOAD=

# Disable browser auto-open (set to true to prevent opening). Legacy alias of LANGFLOW_OPEN_BROWSER
LANGFLOW_OPEN_BROWSER=

# Development mode flag (affects lfx behaviors) true/false
LANGFLOW_DEV=

# Format: true/false to disable tracking of anonymous usage
DO_NOT_TRACK=

# ------------ Third-party / tracing integrations (set only what you use) ------------

# Langsmith / LangChain
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=

# Langfuse tracing
LANGFUSE_SECRET_KEY=
LANGFUSE_PUBLIC_KEY=
LANGFUSE_HOST=

# Traceloop
TRACELOOP_API_KEY=
TRACELOOP_BASE_URL=

# Opik
OPIK_API_KEY=
OPIK_URL_OVERRIDE=
OPIK_WORKSPACE=

# Arize / Phoenix
ARIZE_API_KEY=
ARIZE_SPACE_ID=
ARIZE_COLLECTOR_ENDPOINT=
ARIZE_PHOENIX_BATCH=
PHOENIX_API_KEY=
PHOENIX_COLLECTOR_ENDPOINT=

# LangWatch
LANGWATCH_ENDPOINT=

# OpenAI & Anthropic (common LLM keys used by components)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# ElevenLabs (voice mode)
ELEVENLABS_API_KEY=

# Astra / DataStax enhancements
ASTRA_ENHANCED=

# MongoDB Atlas URI (vector store components)
MONGODB_ATLAS_URI=

# AWS (S3 uploader component)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=

# GitHub token (for template or repo interactions)
GITHUB_TOKEN=

# Value must finish with slash /
#BACKEND_URL=http://localhost:7860/
BACKEND_URL=

# Public base URL used for callbacks/webhooks (ensure trailing slash)
PUBLIC_BASE_URL=

# Request timeouts for load testing harness (locust)
REQUEST_TIMEOUT=
MIN_WAIT=
MAX_WAIT=
